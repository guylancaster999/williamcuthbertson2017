#wrapper 
{
  padding-left: 		0;
  -webkit-transition: 	all 0.5s ease;
  -moz-transition: 		all 0.5s ease;
  -o-transition: 		all 0.5s ease;
  transition: 			all 0.5s ease;
}
#wrapper.toggled 
{
  padding-left: 0px;
}
#sidebar-wrapper 
{
  z-index: 				1000;
  position:	 			fixed;
  left: 				80px;
  height: 				100%;
  margin-left: 			-80px;
  overflow-y: 			auto;
  background: 			#000;
  -webkit-transition: 	all 0.5s ease;
  -moz-transition: 		all 0.5s ease;
  -o-transition: 		all 0.5s ease;
  transition: 			all 0.5s ease;
}
#wrapper.toggled #sidebar-wrapper
 {
  width: 80px;
}
#page-content-wrapper {
  width: 	100%;
  position: absolute;
  padding: 	5px;
}
#wrapper.toggled #page-content-wrapper {
  position: 	absolute;
  margin-right: -80px;
}
.sidebar-nav 
{
  position: absolute;
  top: 0;
  width: 80px;
  margin: 0;
  padding: 0;
  list-style: none;
  padding-left: 0px;
}
.sidebar-nav li {
  list-style: none;
  margin-left: 0;
  padding-left: 0;
}
@media (min-width: 768px) {
  #wrapper {
    padding-left: 80px;
  }
  #wrapper.toggled {
    padding-left: 0;
  }
  #sidebar-wrapper {
    width: 80px;
  }
  #wrapper.toggled #sidebar-wrapper {
    width: 0;
  }
  #page-content-wrapper {
    padding: 10px;
    position: relative;
  }
  #wrapper.toggled #page-content-wrapper {
    position: relative;
    margin-right: 0;
  }
}
